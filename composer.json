    "keywords": ["behat", "contexts"],
    "type": "library",
    "require": {
        "php": ">=5.3.3",
        "pablodip/felpado": "dev-master",
        "behat/behat": "~2.4",
        "symfony/http-foundation": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "mockery/mockery": "~0.8",
        "symfony/process": "~2.0"
    },
    "autoload": {
        "psr-0": { "Akamon\\Behat\\Context": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
